type Hero @rootEntity {
    name: String
    age: Int @defaultValue(value: 42)
    nickNames: [String]
    movies: [Movie]
    skills: [Skill]
    suit: Suit
    morality: Morality
    country: Country @reference
}

type Skill @valueObject {
    description: String
    strength: Float
}

type Movie @childEntity {
    name: String
}

type Suit @entityExtension {
    color: String
}

type Country @rootEntity {
    isoCode: String @key
}

enum Morality {
    GOOD
    EVIL
}
