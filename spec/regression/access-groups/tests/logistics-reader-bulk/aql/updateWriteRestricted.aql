WITH @@files
RETURN (
  FOR v_File1
  IN (
    FOR v_id1
    IN @v_updatedIds1
    LET v_entity1 = DOCUMENT(@@files, v_id1)
    RETURN ((v_entity1.`accessGroup` IN @var1) ? v_entity1 : null)
  )
  RETURN {
    "name": v_File1.`name`
  }
)

