RETURN (
  FOR v_File1
  IN (
    FOR v_file1
    IN (
      FOR v_item1
      IN @@files
      FILTER (v_item1.`accessGroup` IN @var1)
      RETURN v_item1
    )
    FILTER (v_file1._key == @var2)
    REMOVE v_file1
    IN @@files
    RETURN OLD
  )
  RETURN {
    "name": v_File1.`name`
  }
)

