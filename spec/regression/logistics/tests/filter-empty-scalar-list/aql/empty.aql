RETURN {
  "allDeliveries": (
    FOR v_delivery1
    IN @@deliveries
    FILTER (LENGTH(v_delivery1.`serialNumbers`) == @var1)
    SORT (v_delivery1.`deliveryNumber`)
    RETURN {
      "deliveryNumber": v_delivery1.`deliveryNumber`,
      "serialNumbers": (IS_LIST(v_delivery1.`serialNumbers`) ? v_delivery1.`serialNumbers` : [])
    }
  )
}
