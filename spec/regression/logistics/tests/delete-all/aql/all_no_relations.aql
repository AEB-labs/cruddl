RETURN (
  FOR v_country1
  IN (
    FOR v_country2
    IN @@countries
    FILTER (v_country2.`isoCode` == @var1)
    REMOVE v_country2
    IN @@countries
    RETURN OLD
  )
  RETURN {
    "isoCode": v_country1.`isoCode`
  }
)

