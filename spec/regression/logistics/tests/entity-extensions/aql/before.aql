LET v_delivery1 = FIRST((
  FOR v_delivery2
  IN @@deliveries
  FILTER (v_delivery2._key == @var1)
  LIMIT @var2
  RETURN v_delivery2
))
LET v_delivery3 = FIRST((
  FOR v_delivery4
  IN @@deliveries
  FILTER (v_delivery4._key == @var3)
  LIMIT @var4
  RETURN v_delivery4
))
RETURN {
  "withValue": (IS_NULL(v_delivery1) ? null : {
    "dgInfo": {
      "unNumber": v_delivery1.`dgInfo`.`unNumber`,
      "notices": (IS_LIST(v_delivery1.`dgInfo`.`notices`) ? v_delivery1.`dgInfo`.`notices` : [])
    }
  }),
  "withoutValue": (IS_NULL(v_delivery3) ? null : {
    "dgInfo": {
      "unNumber": v_delivery3.`dgInfo`.`unNumber`,
      "notices": (IS_LIST(v_delivery3.`dgInfo`.`notices`) ? v_delivery3.`dgInfo`.`notices` : [])
    }
  })
}
