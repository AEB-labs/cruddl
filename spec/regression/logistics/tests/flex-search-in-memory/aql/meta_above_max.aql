RETURN (FIRST(
  FOR v_item1
  IN (
    FOR v_delivery1
    IN @@flex_view_deliveries
    SEARCH true
    RETURN v_delivery1
  )
  COLLECT WITH COUNT INTO v_count1
  RETURN v_count1
) > @var1)

