RETURN {
  "flexSearchCountries": (
    FOR v_country1
    IN (
      FOR v_country2
      IN @@flex_view_countries
      SEARCH (!(ANALYZER( v_country2.`someKey` IN ( FOR v_token1 IN TOKENS(@var1 , @var2) RETURN v_token1[0] ), @var3 )) && EXISTS(v_country2.`someKey`, "analyzer", @var4))
      RETURN v_country2
    )
    SORT (v_country1.`isoCode`) , (v_country1.`createdAt`)  DESC, (v_country1._key)  DESC
    RETURN {
      "isoCode": v_country1.`isoCode`,
      "someKey": v_country1.`someKey`
    }
  )
}
