    SORT (v_delivery1.`deliveryNumber`)
    RETURN v_delivery1
  )
  UPDATE v_currentEntity1
  WITH {
    "consignee": @var3,
    "updatedAt": @var4
  }
  IN @@deliveries
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
