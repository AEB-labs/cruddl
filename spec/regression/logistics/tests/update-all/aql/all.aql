    FOR v_id1
    IN @v_updatedIds1
    RETURN DOCUMENT(@@deliveries, v_id1)
  )
  RETURN {
    "deliveryNumber": v_Delivery1.`deliveryNumber`,
    "consignee": (IS_NULL(v_Delivery1.`consignee`) ? null : {
      "city": v_Delivery1.`consignee`.`city`
    })
  }
