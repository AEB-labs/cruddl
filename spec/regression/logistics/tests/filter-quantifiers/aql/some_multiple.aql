RETURN {
  "allDeliveries": (
    FOR v_delivery1
    IN @@deliveries
    FILTER (FIRST(
      FOR v_item1
      IN (
        FOR v_items1
        IN (IS_LIST(v_delivery1.`items`) ? v_delivery1.`items` : [])
        FILTER ((v_items1.`quantity` < @var1) && (v_items1.`quantity` > @var2))
        RETURN v_items1
      )
      COLLECT WITH COUNT INTO v_count1
      RETURN v_count1
    ) > @var3)
    SORT (v_delivery1.`deliveryNumber`)
    RETURN {
      "deliveryNumber": v_delivery1.`deliveryNumber`
    }
  )
}
