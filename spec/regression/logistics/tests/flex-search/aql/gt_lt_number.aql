RETURN {
  "flexSearchDeliveries": (
    FOR v_delivery1
    IN (
      FOR v_delivery2
      IN @@flex_view_deliveries
      SEARCH (((v_delivery2.`items`.`quantity` == null) || !(EXISTS(v_delivery2.`items`.`quantity`))) || (v_delivery2.`items`.`quantity` < @var1))
      RETURN v_delivery2
    )
    SORT (v_delivery1.`deliveryNumber`) , (v_delivery1.`createdAt`)  DESC, (v_delivery1._key)  DESC
    RETURN {
      "deliveryNumber": v_delivery1.`deliveryNumber`
    }
  )
}
