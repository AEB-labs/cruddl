RETURN {
  "flexSearchDeliveries": (
    FOR v_delivery1
    IN (
      FOR v_delivery2
      IN @@flex_view_deliveries
      SEARCH (ANALYZER( v_delivery2.`description` IN TOKENS(@var1, @var2),@var3) && ANALYZER( v_delivery2.`description` IN TOKENS(@var4, @var5),@var6))
      RETURN v_delivery2
    )
    SORT (v_delivery1.`deliveryNumber`) , (v_delivery1.`createdAt`)  DESC, (v_delivery1._key)  DESC
    RETURN {
      "deliveryNumber": v_delivery1.`deliveryNumber`
    }
  )
}
