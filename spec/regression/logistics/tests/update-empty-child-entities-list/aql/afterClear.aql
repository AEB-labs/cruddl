LET v_delivery1 = FIRST((
  FOR v_delivery2
  IN @@deliveries
  FILTER (v_delivery2._key == @var1)
  LIMIT @var2
  RETURN v_delivery2
))
RETURN {
  "Delivery": (IS_NULL(v_delivery1) ? null : {
    "items": (
      FOR v_deliveryItem1
      IN (IS_LIST(v_delivery1.`items`) ? v_delivery1.`items` : [])
      RETURN {
        "id": v_deliveryItem1.`id`,
        "itemNumber": v_deliveryItem1.`itemNumber`
      }
    )
  })
}
