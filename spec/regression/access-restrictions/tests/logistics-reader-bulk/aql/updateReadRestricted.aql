        RETURN v_item2
      )
      FILTER (v_file1._key == @var3)
      RETURN v_file1
    )
    FILTER !((MERGE(v_currentEntity1, {
      "name": @var4,
      "updatedAt": @var5
    }).`accessGroup` == @var6))
    RETURN v_currentEntity1
  )
  COLLECT WITH COUNT INTO v_count1
  RETURN v_count1
) == 0)

