RETURN (FIRST(
  FOR v_item1
  IN (
    FOR v_currentEntity1
    IN (
      FOR v_file1
      IN (
        FOR v_item2
        IN @@files
        FILTER ((v_item2.`accessGroup` == @var1) || (v_item2.`accessGroup` == @var2))
        RETURN v_item2
      )
      FILTER (v_file1._key == @var3)
      LIMIT @var4
      RETURN v_file1
    )
    FILTER !((MERGE(v_currentEntity1, {
      "name": @var5,
      "updatedAt": @var6
    }).`accessGroup` == @var7))
    RETURN v_currentEntity1
  )
  COLLECT WITH COUNT INTO v_count1
  RETURN v_count1
) == 0)

