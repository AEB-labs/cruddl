WITH @@files
RETURN (
  FOR v_file1
  IN [
    FIRST(
      LET v_entity1 = DOCUMENT(@@files, @var1)
      RETURN (((v_entity1.`accessGroup` == @var2) || (v_entity1.`accessGroup` == @var3)) ? v_entity1 : null)
    )
  ]
  RETURN {
    "name": v_file1.`name`
  }
)

