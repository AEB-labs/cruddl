RETURN (
  FOR v_currentEntity1
  IN (
    FOR v_file1
    IN (
      FOR v_item1
      IN @@files
      FILTER ((v_item1.`accessGroup` == @var1) || (v_item1.`accessGroup` == @var2))
      RETURN v_item1
    )
    FILTER (v_file1._key == @var3)
    LIMIT @var4
    RETURN v_file1
  )
  UPDATE v_currentEntity1
  WITH {
    "name": @var5,
    "updatedAt": @var6
  }
  IN @@files
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
)

