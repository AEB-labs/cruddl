RETURN (
  FOR v_currentEntity1
  IN (
    FOR v_paper1
    IN @@papers
    FILTER (v_paper1._key == @var1)
    LIMIT @var2
    RETURN v_paper1
  )
  UPDATE v_currentEntity1
  WITH {}
  IN @@papers
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
)

