WITH @@papers
LET v_paper1 = DOCUMENT(@@papers, @var1)
RETURN (IS_NULL(v_paper1) ? null : {
  "key": v_paper1.`key`
})

// --------------------------------

WITH @@papers
RETURN FIRST(
  INSERT @var1 IN @@papers
  RETURN NEW._key
)

