WITH @@deliveries, @@shipments
RETURN (
  UPSERT {_to: CONCAT(@var1, FIRST(@v_updatedIds1))}
  INSERT {_from: @var2, _to: CONCAT(@var3, FIRST(@v_updatedIds1))}
  UPDATE {_from: @var4, _to: CONCAT(@var5, FIRST(@v_updatedIds1))}
  IN @@deliveries_order
