RETURN (
  FOR v_currentEntity1
  IN (
    FOR v_delivery1
    IN @@deliveries
    FILTER (v_delivery1._key == @var1)
    LIMIT @var2
    RETURN v_delivery1
  )
  UPDATE v_currentEntity1
  WITH {}
  IN @@deliveries
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
)

// --------------------------------

WITH @@deliveries
LET v_delivery1 = DOCUMENT(@@deliveries, @var1)
RETURN (IS_NULL(v_delivery1) ? null : {
  "itemCount": FIRST(
    FOR v_item1
    IN v_delivery1.`deliveryContents`[*].`items`[*][**]
    COLLECT WITH COUNT INTO v_count1
    RETURN v_count1
  ),
  "allItems": v_delivery1.`deliveryContents`[*].`items`[*][** RETURN {
    "itemNumber": CURRENT.`itemNumber`
  }]
})

// --------------------------------

WITH @@deliveries
RETURN {
  "updateDelivery": @v_updateDelivery1
}
