WITH @@deliveries, @@invoices
LET v_shipment1 = FIRST((
  FOR v_shipment2
  IN (
    FOR v_item1
    IN @@shipments
    FILTER (v_item1.`accessGroup` IN @var1)
    RETURN v_item1
  )
  FILTER (v_shipment2.`shipmentNumber` == @var2)
  LIMIT @var3
  RETURN v_shipment2
))
RETURN {
  "Shipment": (IS_NULL(v_shipment1) ? null : {
    "allInvoices": (
      FOR v_node1, v_edge1, v_path1 IN @var4..@var5 OUTBOUND v_shipment1 @@shipments_deliveries
      FILTER (v_node1.`accessGroup` IN @var6)
      FOR v_node2, v_edge2, v_path2 IN @var7..@var8 OUTBOUND v_node1 @@deliveries_invoices
      FILTER (v_node2.`accessGroup` IN @var9)
      FILTER v_node2._key != null
      SORT (v_node2.`invoiceNumber`)
      RETURN {
        "number": v_node2.`invoiceNumber`
      }
    )
  })
}
