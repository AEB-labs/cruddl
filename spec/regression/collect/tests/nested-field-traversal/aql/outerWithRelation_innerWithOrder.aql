WITH @@deliveries
LET v_shipment1 = FIRST((
  FOR v_shipment2
  IN @@shipments
  FILTER (v_shipment2.`shipmentNumber` == @var1)
  LIMIT @var2
  RETURN v_shipment2
))
RETURN {
  "Shipment": (IS_NULL(v_shipment1) ? null : {
    "allItems": (
      FOR v_root1, v_edge1, v_path1 IN @var3..@var4 OUTBOUND v_shipment1 @@shipments_deliveries
      FILTER v_root1._key != null
      FOR v_item1 IN v_root1.`deliveryContents`[*].`items`[*][**]
      RETURN {
        "subSubItems": (
          FOR v_item2
          IN v_item1.`subItems`[*].`subItems`[*][**]
          SORT (v_item2.`itemNumber`)  DESC
          RETURN {
            "itemNumber": v_item2.`itemNumber`
          }
        )
      }
    )
  })
}

