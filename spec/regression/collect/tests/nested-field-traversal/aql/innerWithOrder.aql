LET v_delivery1 = FIRST((
  FOR v_delivery2
  IN @@deliveries
  FILTER (v_delivery2.`deliveryNumber` == @var1)
  LIMIT @var2
  RETURN v_delivery2
))
RETURN {
  "Delivery": (IS_NULL(v_delivery1) ? null : {
    "allItems": (
      FOR v_item1
      IN v_delivery1.`deliveryContents`[*].`items`[*][**]
      RETURN {
        "subSubItems": (
          FOR v_item2
          IN v_item1.`subItems`[*].`subItems`[*][**]
          SORT (v_item2.`itemNumber`)  DESC
          RETURN {
            "itemNumber": v_item2.`itemNumber`
          }
        )
      }
    )
  })
}

