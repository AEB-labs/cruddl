RETURN (
  FOR v_currentEntity1
  IN (
    FOR v_hero1
    IN @@heroes
    SORT (v_hero1._key)
    LIMIT @var1
    RETURN v_hero1
  )
  UPDATE v_currentEntity1
  WITH {
    "name": @var2,
    "updatedAt": @var3
  }
  IN @@heroes
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
)

