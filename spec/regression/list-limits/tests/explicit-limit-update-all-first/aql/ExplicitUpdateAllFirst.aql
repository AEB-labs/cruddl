WITH @@heroes
RETURN (
  FOR v_hero1
  IN (
    FOR v_id1
    IN @v_updatedIds1
    RETURN DOCUMENT(@@heroes, v_id1)
  )
  RETURN {
    "name": v_hero1.`name`
  }
)

