WITH @@roots
LET v_super1 = FIRST((
  FOR v_super2
  IN @@supers
  FILTER (v_super2.`key` == @var1)
  LIMIT @var2
  RETURN v_super2
))
RETURN {
  "Super": (IS_NULL(v_super1) ? null : {
    "roots": (
      FOR v_root1
      IN OUTBOUND v_super1 @@supers_roots
      FILTER v_root1 != null
      RETURN {
        "key": v_root1.`key`
      }
    )
  })
}
