RETURN {
  "foundation": {
    "allCountries": (
      FOR v_country1
      IN @@countries
      SORT (v_country1.`isoCode`)
      RETURN {
        "id": v_country1._key,
        "isoCode": v_country1.`isoCode`,
        "description": v_country1.`description`[* RETURN {
          "languageIsoCode": CURRENT.`languageIsoCode`,
          "translation": CURRENT.`translation`
        }]
      }
    )
  }
}

