LET v_delivery1 = FIRST((
  FOR v_delivery2
  IN @@deliveries
  FILTER (v_delivery2._key == @var1)
  LIMIT @var2
  RETURN v_delivery2
))
RETURN {
  "logistics": {
    "delivery": {
      "Delivery": (IS_NULL(v_delivery1) ? null : {
        "deliveryNumber": v_delivery1.`deliveryNumber`
      })
    }
  }
}
