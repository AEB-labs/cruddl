WITH @@dependents
RETURN (
  FOR v_to1 IN [@var1]
  FOR v_edge1 IN @@mains_additionalDependent
  FILTER v_edge1._to == v_to1
  REMOVE v_edge1 IN @@mains_additionalDependent
)

