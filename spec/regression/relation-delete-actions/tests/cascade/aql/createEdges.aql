WITH @@dependents
RETURN (
  UPSERT {_from: CONCAT(@var1, FIRST(@v_updatedIds1))}
  INSERT {_from: CONCAT(@var2, FIRST(@v_updatedIds1)), _to: @var3}
  UPDATE {_from: CONCAT(@var4, FIRST(@v_updatedIds1)), _to: @var5}
  IN @@mains_additionalDependent
)

