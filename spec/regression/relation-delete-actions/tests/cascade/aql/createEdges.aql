RETURN (
  FOR v_currentEntity1
  IN (
    FOR v_main1
    IN @@mains
    FILTER (v_main1._key == @var1)
    LIMIT @var2
    RETURN v_main1
  )
  UPDATE v_currentEntity1
  WITH {}
  IN @@mains
  OPTIONS { mergeObjects: false }
  RETURN NEW._key
)

